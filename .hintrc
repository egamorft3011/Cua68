{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "axe/forms": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "height: fit-content",
          "user-select"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}